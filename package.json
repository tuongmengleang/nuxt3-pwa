{
  "name": "nuxt3-boilerplate",
  "version": "1.0.0",
  "description": "Nuxt 3 starter boilerplate with a lot of features. Nuxt3 + Tailwindcss3 + VueUse + Vuei18n",
  "repository": {
    "type": "git",
    "url": "git://github.com/tuongmengleang/nuxt3-boilerplate"
  },
  "author": "Tuong Mengleang <leang.dev@gmail.com>",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "start": "node .output/server/index.mjs",
    "serve": "serve dist/",
    "clean": "rm -rf .nuxt dist .output",
    "remove": "rm -rf node_modules yarn.lock",
    "generate:locales": "node tools/translator.js ./locales en.yml"
  },
  "devDependencies": {
