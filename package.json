    "babel-eslint": "^10.1.0",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-nuxt": "^3.2.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.3.0",
    "js-yaml": "^4.1.0",
    "nuxt": "3.0.0-rc.8",
    "nuxt-windicss": "^2.5.0",
    "pinia": "^2.0.18",
    "postcss": "^8.4.14",
    "postcss-loader": "^7.0.1",
    "prettier": "^2.7.1",
    "sass": "^1.54.0",
    "sass-loader": "^13.0.2",
    "translate": "^1.4.1",
